    "@testing-library/react": "^14.0.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
